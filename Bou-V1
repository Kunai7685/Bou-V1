	Text.Size = UDim2.new(0.878341854, 0, 0.559999943, 0)
	Text.Font = Enum.Font.FredokaOne
	Text.Text = "Description"
	Text.TextColor3 = Color3.fromRGB(208, 208, 208)
	Text.TextScaled = true
