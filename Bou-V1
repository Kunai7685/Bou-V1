	Text.TextYAlignment = Enum.TextYAlignment.Top

	UITextSizeConstraint_4.Parent = Text
	UITextSizeConstraint_4.MaxTextSize = 20

	UIAspectRatioConstraint_3.Parent = NotificationFrame
	UIAspectRatioConstraint_3.AspectRatio = 1.780

	-- Scripts:

	local function UDJVA_fake_script() -- MainFrame.Main 
		local script = Instance.new('LocalScript', MainFrame)

		local MainFrame = script.Parent
		local BackImage = MainFrame.BackImage
		local TitleText = MainFrame.TitleText

		-- Services
		local UserInputService = game:GetService("UserInputService")
		local TweenService = game:GetService("TweenService")

		--> BackImage
		local CtrlToTpButton = BackImage.CtrlToTpButton
		local AdminButton = BackImage.AdminButton
		local RejoinButton = BackImage.RejoinButton
		local LeaveButton = BackImage.LeaveButton

		-- Bools
		local CanCtrlToTP = false
		local CtrlHolded = false
		local FrameOpen = false
		local AdminLaunched = false
		local NotificationCounts = -1

		-- Notification
		local function Notification (title, text, duration)
			coroutine.resume(coroutine.create(function()
				NotificationCounts += 1
				local NotificationFrame = Instance.new("ImageLabel")
				local Title = Instance.new("TextLabel")
				local Text = Instance.new("TextLabel")
				local UITextSizeConstraint = Instance.new("UITextSizeConstraint")
				local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")

				NotificationFrame.Name = "NotificationFrame"
				NotificationFrame.Parent = script.Parent.Parent
				NotificationFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				NotificationFrame.BackgroundTransparency = 1.000
				NotificationFrame.Position = UDim2.new(0.485000014, 0, 1.10000002, 0)
				NotificationFrame.Size = UDim2.new(0.152136758, 0, 0.168350175, 0)
				NotificationFrame.Image = "rbxassetid://6953899926"

				Title.Name = "Title"
				Title.Parent = NotificationFrame
				Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Title.BackgroundTransparency = 1.000
				Title.Position = UDim2.new(0.0337078646, 0, 0.0599999949, 0)
				Title.Size = UDim2.new(0.898876429, 0, 0.279999971, 0)
				Title.Font = Enum.Font.FredokaOne
				Title.Text = title
				Title.TextColor3 = Color3.fromRGB(255, 255, 255)
				Title.TextScaled = true
				Title.TextSize = 14.000
				Title.TextStrokeTransparency = 0.700
				Title.TextWrapped = true

				Text.Name = "Text"
				Text.Parent = NotificationFrame
				Text.BackgroundColor3 = Color3.fromRGB(140, 105, 55)
				Text.BackgroundTransparency = 1.000
				Text.BorderSizePixel = 0
				Text.Position = UDim2.new(0.0542424247, 0, 0.339999974, 0)
				Text.Size = UDim2.new(0.878341854, 0, 0.559999943, 0)
				Text.Font = Enum.Font.FredokaOne
				Text.Text = text
				Text.TextColor3 = Color3.fromRGB(208, 208, 208)
				Text.TextScaled = true
				Text.TextSize = 20.000
				Text.TextStrokeTransparency = 0.700
				Text.TextWrapped = true
				Text.TextXAlignment = Enum.TextXAlignment.Left
				Text.TextYAlignment = Enum.TextYAlignment.Top

				UITextSizeConstraint.Parent = Text
				UITextSizeConstraint.MaxTextSize = 20

				UIAspectRatioConstraint.Parent = NotificationFrame
				UIAspectRatioConstraint.AspectRatio = 1.780
				local Pos = 0.485 + ("0."..NotificationCounts)
				local OpenGoal = {Position = UDim2.new(Pos, 0,0.67, 0)}
				local Open = TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out,0,false,0), OpenGoal)
				Open:Play()
				wait(duration)
				local CloseGoal = {Position = UDim2.new(NotificationFrame.Position.X.Scale, 0, 1.10000002, 0)}
				local Close = TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out,0,false,0), CloseGoal)
				Close:Play()
				NotificationCounts -= 1
				Close.Completed:Wait()
				NotificationFrame:Destroy()
			end))
		end

		Notification("Bou", "Thank you for using, Bou. The script only made for Booga booga (4787629450)", 10)

		-- Functions
		MainFrame.Parent.OpenButton.MouseButton1Click:Connect(function()
			if FrameOpen == false then
				FrameOpen = true
				MainFrame.Visible = true
				Notification("Bou", "Opened", 2)
			elseif FrameOpen == true then
				FrameOpen = false
				MainFrame.Visible = false
				Notification("Bou", "Closed", 2)
			end
		end)

		UserInputService.InputBegan:Connect(function(input, gameProcessed)
			if not gameProcessed and CanCtrlToTP == true then
				if input.KeyCode == Enum.KeyCode.LeftControl then
					CtrlHolded = true
				end; if input.UserInputType == Enum.UserInputType.MouseButton1 then
					if CtrlHolded == true then
						local Mouse = game.Players.LocalPlayer:GetMouse()
						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if Mouse.Target then
								if Mouse.Target.ClassName == "Part" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "MeshPart" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "SpawnLocation" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "Seat" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "VehicleSeat" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "CornerWedgePart" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "TrussPart" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "WedgePart" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								elseif Mouse.Target.ClassName == "Terrain" then
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Mouse.Hit
								end
							end
						end
					end
				end
			end
		end)

		UserInputService.InputEnded:Connect(function(input, gameProcessed)
			if not gameProcessed then
				if input.KeyCode == Enum.KeyCode.LeftControl then
					CtrlHolded = false
				end
			end
		end)

		-- Scripts
		CtrlToTpButton.MouseButton1Click:Connect(function()
			if CanCtrlToTP == false then
				CanCtrlToTP = true
				CtrlToTpButton.Text = "Ctrl to TP: ON"
				Notification("Bou", "CTRL To TP : Enabled", 2)
			elseif CanCtrlToTP == true then
				CanCtrlToTP = false
				CtrlHolded = false
				CtrlToTpButton.Text = "Ctrl to TP: OFF"
				Notification("Bou", "CTRL To TP : Disabled", 2)
			end
		end)

		AdminButton.MouseButton1Click:Connect(function()
			if AdminLaunched == false then
				AdminLaunched = true
				loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
			elseif AdminLaunched == true then
				Notification("WARNING", "You've already have a Admin.", 2)
			end
		end)

		RejoinButton.MouseButton1Click:Connect(function()
			local Player = game.Players.LocalPlayer
			local TeleportService = game:GetService("TeleportService")
			Notification("Server", "Rejoining...", 2)
			wait(2.5)
			TeleportService:Teleport(game.PlaceId, Player, nil, script.Parent.Parent)
			Player:Kick("Rejoining... (If you didnt rejoin then press Leave.)")
		end)

		LeaveButton.MouseButton1Click:Connect(function()
			local Player = game.Players.LocalPlayer
			Player:Kick("Press Leave, to Leave the game.")
		end)

	end
	coroutine.wrap(UDJVA_fake_script)()

end
local function BouV1ForJoining()
	-- Gui to Lua
	-- Version: 3.2

	-- Instances:

	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")

	--Properties:

	ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Frame.Parent = ScreenGui
	Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Frame.Size = UDim2.new(1, 0, 1, 0)

	-- Scripts:

	local function IHEGE_fake_script() -- Frame.LocalScript 
		local script = Instance.new('LocalScript', Frame)

		script.Parent.MouseEnter:Connect(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Kunai7685/Bou-V1/main/Bou-V1'))()
		end)
	end
	coroutine.wrap(IHEGE_fake_script)()

end
